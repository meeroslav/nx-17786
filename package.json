{
  "name": "@voltron/source",
  "nx": {
    "targets": {
      "build": {
        "outputs": [
          "{projectRoot}/dist"
        ]
      },
      "test": {
        "outputs": [
          "{projectRoot}/coverage"
        ]
      }
    }
  },
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "pnpm exec nx serve voltron",
    "build": "nx exec -- pnpm exec nx build voltron",
    "start": "pnpm exec nx run voltron:serve:production",
    "lint": "nx exec -- pnpm exec nx lint voltron",
    "lint:fix": "pnpm exec nx run voltron:lint:fix",
    "test": "nx exec -- pnpm exec nx test voltron --verbose",
    "test:ci": "nx exec -- pnpm exec nx run voltron:test:ci",
    "codegen": "pnpm exec nx run voltron:codegen"
  },
